<p id="notice"><%= notice %></p>

<h1>Stores Books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores_books.each do |stores_book| %>
      <tr>
        <td><%= link_to 'Show', stores_book %></td>
        <td><%= link_to 'Edit', edit_stores_book_path(stores_book) %></td>
        <td><%= link_to 'Destroy', stores_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stores Book', new_stores_book_path %>
